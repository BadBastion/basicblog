<% include defaults/header %>

<div id="form">

  <form action="<%-form_target %>" method="post">
    <input type="hidden" name="id" value="<%-form_id %>" required />
    <p>Title</p>
    <input type="text" name="title" value="<%-form_title %>" required />
    <p>Content</p>
    <textarea name="content" rows="30" cols="100" required ><%-form_content%></textarea>
    <input type="submit" >
  </form>

</div>


<% include defaults/footer %>
